<launch>

  <!-- calibration file path -->
  <arg name="velodyne_calib" default="$(find velodyne_pointcloud)/params/VLP16db.yaml"/>
  <!--arg name="camera_calib" default="$(env HOME)/.autoware/data/calibration/camera_lidar_3d/prius/nic-150407.yml"/-->

  <!-- VLP-16 front -->
  <include file="$(find velodyne_pointcloud)/launch/VLP16_points.launch">
    <arg name="calibration" value="$(arg velodyne_calib)"/>
    <arg name="device_ip" value="192.168.1.201"/>
    <arg name="frame_id" value="vlp201"/>
    <arg name="port" value="2368"/>
  </include>

  <!-- VLP-16 rear -->
  <include file="$(find velodyne_pointcloud)/launch/VLP16_points.launch">
    <arg name="calibration" value="$(arg velodyne_calib)"/>
    <arg name="device_ip" value="192.168.1.202"/>
    <arg name="frame_id" value="vlp202"/>
    <arg name="port" value="2268"/>
  </include>

  <!-- Novatel GNSS -->
  <include file="$(find novatel_gps_driver)/launch/tester_for_eth.launch"/>

  <!-- PointGrey Grasshopper3 -->
  <!--include file="$(find autoware_pointgrey_drivers)/scripts/grasshopper3.launch">
    <arg name="CalibrationFile" value="$(arg camera_calib)"/>
  </include-->

</launch>
